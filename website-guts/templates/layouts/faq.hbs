---
layout: wrapper
layout_script: false
layout_body_class: false
faq_navigation:
-
 text: "Getting Started"
 url: "/gettingstarted"
-
 text: "Webinar Signup"
 url: "/resources/live-demo-webinar"
-
 text: "FAQ"
 url: "/faq"
-
 text: "Knowledge Base"
 url: "https://help.optimizely.com"
-
 text: "API Documentation"
 url: "http://developers.optimizely.com"
-
 text: "Blog"
 url: "http://blog.optimizely.com"
 swap_id: "faq-subnav-blog"
---
<div id="main">
    <div id="page-header">
        <div class="container">
            <div class="clearfix page-subheader-cont">
                <h1>{{this.visible_title}}</h1>
                <ul class="sub-header-nav">
                    {{#each faq_navigation}}
                    <li{{#if swap_id}} class="SL_swap" id="{{swap_id}}"{{/if}}><a href="{{#isUrl url}}{{../../linkPath}}{{/isUrl}}{{url}}">{{text}}</a></li>
                    {{/each}}
                </ul>
            </div><!---->
        </div><!--/.container-->
    </div><!--/#page-header-->
    <div id="page-header-gradient"></div>
    <div id="page-content">
        {{#unless this.no_container}}<div class="container">{{/unless}}
            {% body %}
        {{#unless this.no_container}}</div><!--/.container-->{{/unless}}
    </div><!--/#content-->
</div><!--/#main-->
