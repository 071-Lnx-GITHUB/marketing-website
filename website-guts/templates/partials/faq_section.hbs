<section class="faq-section">
{{#with global_faq_section}}
  <div class="faq-container">
    {{#each faqs}}
    <p class="question">{{question}}</p>
    <p class="answer">{{{answer}}}</p>
    {{/each}}
  </div>
  <!--<p class="contact-text">{{contact.text}}</p>-->
  <div id="phone-numbers-list" class="phone-container"> 
    <p class="phone-number">
    US: <a href="tel:{{contact.us_phone_number}}">{{contact.us_phone_number}}</a><br>
    UK: <a href="tel:{{contact.uk_phone_number}}">{{contact.uk_phone_number}}</a><br>
    NL: <a href="tel:{{contact.nl_phone_number}}">{{contact.nl_phone_number}}</a><br>
    DE: <a href="tel:{{contact.de_phone_number}}">{{contact.de_phone_number}}</a><br>
    FR: <a href="tel:{{contact.fr_phone_number}}">{{contact.fr_phone_number}}</a><br>
    ES: <a href="tel:{{contact.es_phone_number}}">{{contact.es_phone_number}}</a><br>
    JP: <a href="tel:{{contact.jp_phone_number}}">{{contact.jp_phone_number}}</a>
    </p>
  </div>
{{/with}}
</section>
