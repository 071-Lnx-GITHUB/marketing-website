<section id="technology-hero" class="panel hero">
  {{#with page_data.intro}}
    <h1>{{{headline}}}</h1>
    <h2>{{{subhead}}}</h2>
  {{/with}}
</section>

<section id="technology-blurb" class="panel">
  {{#with page_data.intro}}
    {{{blurb}}}
  {{/with}}
</section>

<section  id="technology-why"  class="why-items container">
  <h3 class="description-title">{{why.title}}</h3>
  <div class="tracks-container flex-container">
    {{#each why.items.list}}
      <div class="why-item-container {{this.class}}">
        <p class="item-title adelle-sans--bold">{{this.title}}</p>
        <p class="detail-text">{{this.blurb}}</p>
        <ul>
          <li>{{this.list_bullet}}</li>
        </ul>
      </div>
    {{/each}}
  </div>
</section>


