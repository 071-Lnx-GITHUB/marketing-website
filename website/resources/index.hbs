---
layout: resources.hbs
seo_title: Resources
visible_title: "Resources"
no_container: true
sub_head: Everything you need to learn about Optimization
page_script: resources-page.js
body_class:
- resources-page
script_dependencies:
- "//cdnjs.cloudflare.com/ajax/libs/jquery.isotope/2.0.0/isotope.pkgd.min.js"
---

<div class="container">
  <div class="row">
    <div class="resources-grid">
      {{#resources}}
        {{#each pages}}
          <div class="resources-grid-elm {{#toLowerCase data.type}}{{/toLowerCase}}">
            <div class="resources-image">
              <a href="{{data.url}}"><img src="{{data.photo}}"></a>
            </div>
            <div class="resources-title"><a href="{{data.url}}">{{data.title}}</a></div>
            <div class="resource-type">{{data.type}}</div>
          </div>
        {{/each}}
      {{/resources}}
    </div><!-- /.resources-grid -->
    <div class="integrations-message">
      <p>{{{resources-page.filter.no_results}}}</p>
    </div>
  </div><!-- /.row-->
</div><!-- /.container-->
